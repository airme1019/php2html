{
  "name": "php2html",
  "version": "0.2.4",
  "description": "Convert php files to html",
  "license": "MIT",
  "repository": "bezoerb/php2html",
  "author": {
    "name": "Ben ZÃ¶rb",
    "email": "ben@sommerlaune.com",
    "url": "sommerlaune.com"
  },
  "engines": {
    "node": ">=0.12.0"
  },
  "scripts": {
    "test": "xo && npm run prepublish && nyc ava",
    "prepublish": "babel src --out-dir=dist",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "preferGlobal": true,
  "main": "dist/index.js",
  "bin": "dist/cli.js",
  "files": [
    "dist"
  ],
  "keywords": [
    "php",
    "convert",
    "html"
  ],
  "dependencies": {
    "async": "^1.5.2",
    "bluebird": "^3.2.2",
    "chalk": "^1.0.0",
    "connect": "^3.3.5",
    "connect-modrewrite": "^0.9.0",
    "debug": "^2.2.0",
    "gateway": "^1.0.0",
    "get-port": "^2.1.0",
    "get-stdin": "^5.0.1",
    "htmlhint": "^0.9.7",
    "indent-string": "^2.1.0",
    "lodash": "^4.3.0",
    "meow": "^3.1.0",
    "qs": "^6.1.0",
    "read-pkg-up": "^1.0.1",
    "request": "^2.55.0",
    "shelljs": "^0.7.0",
    "tempfile": "^1.1.0",
    "tmp": "0.0.28",
    "update-notifier": "^0.6.0"
  },
  "devDependencies": {
    "ava": "^0.14.0",
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "coveralls": "^2.11.9",
    "mockery": "^1.4.0",
    "normalize-newline": "^2.0.0",
    "nyc": "^6.4.2",
    "pify": "^2.3.0",
    "read-package-json": "^2.0.3",
    "xo": "^0.15.0"
  },
  "xo": {
    "space": 4,
    "ignores": [
      "index.js",
      "cli.js"
    ]
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "retainLines": true
  }
}
